<ul>
  <h3>A list of 20 titles of movies</h3>
  <p><b>:</b> Retrieve a movie data with a click of each title.</p>
  <% results.forEach(movie => { %>
    <li>
      <a href="/movies/<%= movie._id %>" style="display:flex; list-style-type:none; text-decoration:none;">
        <span><%= movie.title %></span>
      </a>
    </li>
  <% }) %>
</ul>